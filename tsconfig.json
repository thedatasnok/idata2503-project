{
  "extends": "expo/tsconfig.base",
  "include": [
    "src",
    "app.config.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "compilerOptions": {
    "strict": true,
    "module": "Node16",
    "moduleResolution": "Node16",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}
